<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">

  <welcome-file-list>
    <welcome-file>/index.jsp</welcome-file>
   </welcome-file-list>
 
<servlet>
<servlet-name>create</servlet-name>
<servlet-class>servlets.Create</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>create</servlet-name>
<url-pattern>/create.jsp</url-pattern>
</servlet-mapping>


<servlet>
<servlet-name>update</servlet-name>
<servlet-class>servlets.Update</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>update</servlet-name>
<url-pattern>/update.jsp</url-pattern>
</servlet-mapping>


<servlet>
<servlet-name>delete</servlet-name>
<servlet-class>servlets.Delete</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>delete</servlet-name>
<url-pattern>/delete.jsp</url-pattern>
</servlet-mapping>


<servlet>
<servlet-name>search</servlet-name>
<servlet-class>servlets.Search</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>search</servlet-name>
<url-pattern>/search.jsp</url-pattern>
</servlet-mapping>

</web-app>